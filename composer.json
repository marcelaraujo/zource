{
    "name": "zource/zource",
    "description": "The open source company management application.",
    "license": "MIT",
    "require": {
        "php": "^5.5 || ^7.0",
        "doctrine/doctrine-orm-module": "^0.10.0",
        "doctrine/migrations": "^1.4",
        "ocramius/proxy-manager": "^1.0",
        "zendframework/zendframework": "^2.5.3",
        "zfcampus/zf-apigility": "~1.1",
        "zfcampus/zf-apigility-documentation": "^1.0.5",
        "zfcampus/zf-development-mode": "~2.0",
        "zfcampus/zf-http-cache": "^1.2",
        "zfr/zfr-cors": "^1.2",
        "zendframework/zend-developer-tools": "^0.0.2",
        "zfcampus/zf-apigility-documentation-swagger": "^1.1",
        "ramsey/uuid-doctrine": "^1.2",
        "ua-parser/uap-php": "^3.4",
        "bacon/bacon-qr-code": "^1.0",
        "spomky-labs/otphp": "^7.0",
        "swiftmailer/swiftmailer": "^5.4"
    },
    "require-dev": {
        "zfcampus/zf-apigility-admin": "^1.4.1",
        "zfcampus/zf-apigility-admin-ui": "^1.2.3",
        "zfcampus/zf-deploy": "~1.0",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-0": {
            "ZourceApplication\\": "module/ZourceApplication/src/",
            "ZourceCalendar\\": "module/ZourceCalendar/src/",
            "ZourceContact\\": "module/ZourceContact/src/",
            "ZourceIssue\\": "module/ZourceIssue/src/",
            "ZourceProject\\": "module/ZourceProject/src/",
            "ZourceUser\\": "module/ZourceUser/src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "ZourceApplicationTest\\": "module/ZourceApplication/tests/",
            "ZourceCalendarTest\\": "module/ZourceCalendar/tests/",
            "ZourceContactTest\\": "module/ZourceContact/tests/",
            "ZourceIssueTest\\": "module/ZourceIssue/tests/",
            "ZourceProjectTest\\": "module/ZourceProject/tests/",
            "ZourceUserTest\\": "module/ZourceUser/tests/"
        }
    }
}
