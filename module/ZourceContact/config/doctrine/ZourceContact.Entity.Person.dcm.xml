<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:gedmo="http://gediminasm.org/schemas/orm/doctrine-extensions-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="ZourceContact\Entity\Person" table="contact_person">

        <field name="gender" type="integer" />
        <field name="prefix" type="string" nullable="true" />
        <field name="firstName" type="string" />
        <field name="phoneticFirstName" type="string" nullable="true" />
        <field name="middleName" type="string" nullable="true" />
        <field name="phoneticMiddleName" type="string" nullable="true" />
        <field name="lastName" type="string" />
        <field name="phoneticLastName" type="string" nullable="true" />
        <field name="maidenName" type="string" nullable="true" />
        <field name="suffix" type="string" nullable="true" />
        <field name="nickname" type="string" nullable="true" />
        <field name="jobTitle" type="string" nullable="true" />
        <field name="department" type="string" nullable="true" />
        <field name="company" type="string" nullable="true" />
    </entity>
</doctrine-mapping>
