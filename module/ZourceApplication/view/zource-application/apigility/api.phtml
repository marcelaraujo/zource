<?php

$api = $this->api;

$this->layout('layout/page-header');

$this->placeholder('layout')->pageTitle = $api->getName() . ' (v' . $this->escapeHtml($api->getVersion()) . ')';

foreach ($api->getServices() as $service) {
    echo $this->render('zource-application/apigility/service', array('service' => $service, 'embedded' => true));
}
